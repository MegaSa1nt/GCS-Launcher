{
  "build": {
    "beforeDevCommand": "",
    "beforeBuildCommand": "",
    "frontendDist": "../src"
  },
  "bundle": {
    "active": true,
    "shortDescription": "Лаунчер для GreenCatsServer.",
    "longDescription": "Лаунчер для GreenCatsServer.",
    "copyright": "GreenCatsServer",
    "publisher": "MegaSa1nt, GreenCatsServer",
    "icon": [
      "icons/lil.ico"
    ],
    "windows": {
      "webviewInstallMode": {
        "type": "embedBootstrapper",
        "silent": false
      },
      "nsis": {
        "installerIcon": "icons/updater.ico",
        "languages": [
          "English"
        ],
        "displayLanguageSelector": false
      },
      "allowDowngrades": false
    },
    "targets": "all"
  },
  "productName": "GCS-Client",
  "version": "1.5.2",
  "identifier": "sa1nt.gcs",
  "plugins": {},
  "app": {
    "windows": [
      {
        "transparent": true,
        "fullscreen": false,
        "resizable": false,
        "title": "GreenCatsServer!",
        "width": 720,
        "height": 680,
        "center": true,
        "theme": "Dark",
        "decorations": false,
        "additionalBrowserArgs": "--disable-features=msWebOOUI,msPdfOOUI,msSmartScreenProtection"
      }
    ],
    "withGlobalTauri": true,
    "security": {
      "assetProtocol": {
        "enable": true,
        "scope": [
          "**"
        ]
      },
      "csp": null
    }
  }
}